// can't use jp,,,,

// SublimeSocketAsset's testSuite

// with linefeed
defineFilter:
{
    "name": "13/12/04 18:16:49",
    "patterns": [
        {
            "(.*)dummyline": {
                "selectors": [
                    {
                        "showAtLog": {
                            "message": "should be 1/2:groups[0] in filterSource[]/."
                        }
                    }
                ]
            }
        },
        {
            "^dummyline": {
                "selectors": [
                    {
                        "showAtLog": {
                            "message": "dummy"
                        }
                    }
                ]
            }
        }
    ],
    "dotall":true
}
->filtering:
{
    "name": "13/12/04 18:16:49",
    "source": "1\\n2 dummyline",
    "debug":false
}
->assertResult:
{
    "contains": {
        "filtering": "13/12/04 18:16:49"
    },
    "message": "not match."
}

// no linefeed
